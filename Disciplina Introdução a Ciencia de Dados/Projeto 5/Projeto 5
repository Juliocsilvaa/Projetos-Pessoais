pd.DataFrame(resultados)\n","banco_de_dados_arrumado['Ano']=banco_de_dados_arrumado['Data'].dt.year\n","banco_de_dados_arrumado['Mes']=banco_de_dados_arrumado['Data'].dt.month\n","banco_de_dados_arrumado"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":424},"id":"8sDtwMdSov5-","executionInfo":{"status":"ok","timestamp":1748824800211,"user_tz":180,"elapsed":910,"user":{"displayName":"Julio Silva","userId":"12613495337539221303"}},"outputId":"3cfc5e96-0c34-4ddf-aaf7-aca6d265ebdf"},"execution_count":4,"outputs":[{"output_type":"execute_result","data":{"text/plain":["            Data  Valor   Ano  Mes\n","0     1994-06-01    1.0  1994    6\n","1     1994-06-02   66.3  1994    6\n","2     1994-06-03   25.2  1994    6\n","3     1994-06-04    9.0  1994    6\n","4     1994-06-05   14.3  1994    6\n","...          ...    ...   ...  ...\n","27547 1912-12-27    0.0  1912   12\n","27548 1912-12-28    0.0  1912   12\n","27549 1912-12-29    0.0  1912   12\n","27550 1912-12-30    0.0  1912   12\n","27551 1912-12-31    0.0  1912   12\n","\n","[27552 rows x 4 columns]"],"text/html":["\n","  <div id=\"df-05986ef0-5f27-466b-84c4-01216214934e\" class=\"colab-df-container\">\n","    <div>\n","<style scoped>\n","    .dataframe tbody tr th:only-of-type {\n","        vertical-align: middle;\n","    }\n","\n","    .dataframe tbody tr th {\n","        vertical-align: top;\n","    }\n","\n","    .dataframe thead th {\n","        text-align: right;\n","    }\n","</style>\n","<table border=\"1\" class=\"dataframe\">\n","  <thead>\n","    <tr style=\"text-align: right;\">\n","      <th></th>\n","      <th>Data</th>\n","      <th>Valor</th>\n","      <th>Ano</th>\n","      <th>Mes</th>\n","    </tr>\n","  </thead>\n","  <tbody>\n","    <tr>\n","      <th>0</th>\n","      <td>1994-06-01</td>\n","      <td>1.0</td>\n","      <td>1994</td>\n","      <td>6</td>\n","    </tr>\n","    <tr>\n","      <th>1</th>\n","      <td>1994-06-02</td>\n","      <td>66.3</td>\n","      <td>1994<